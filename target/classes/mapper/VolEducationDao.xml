<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shizhe.dao.VolEducationDao">
  <resultMap id="BaseVolEducationMap" type="VolEducation">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="school_name" jdbcType="VARCHAR" property="schoolName" />
    <result column="project" jdbcType="VARCHAR" property="project" />
    <result column="degree" jdbcType="VARCHAR" property="degree" />
    <result column="start_dt" jdbcType="DATE" property="startDt" />
    <result column="end_dt" jdbcType="VARCHAR" property="endDt" />
    <result column="content" jdbcType="VARCHAR" property="content" />
    <result column="user_id" jdbcType="INTEGER" property="userId" />
  </resultMap>
  <sql id="Base_Column_List">
    id, school_name, project, `degree`, start_dt, end_dt, user_id,content
  </sql>
  <select id="selectByCondition" resultMap="BaseVolEducationMap">
    select
    <include refid="Base_Column_List" />
    from vol_education
    <where>
      1 = 1
      <if test="userId != null and userId != ''">
        AND user_id = #{userId}
      </if>
    </where>
  </select>
  <delete id="deleteById" parameterType="java.lang.Integer">
    delete from vol_education
    where id = #{id}
  </delete>
  <insert id="insertVolEducation" useGeneratedKeys="true" keyProperty="id" parameterType="shizhe.bean.VolEducation">
    insert into vol_education (school_name, project,
      `degree`, start_dt, end_dt, 
      user_id,content)
    values (#{schoolName}, #{project},
      #{degree}, #{startDt}, #{endDt}, 
      #{userId},#{content})
  </insert>
  <update id="updateVolEducation" parameterType="shizhe.bean.VolEducation">
    update vol_education
    set school_name = #{schoolName},
      project = #{project},
      `degree` = #{degree},
      start_dt = #{startDt},
      end_dt = #{endDt},
      user_id = #{userId},
      content=#{content}
    where id = #{id}
  </update>
</mapper>